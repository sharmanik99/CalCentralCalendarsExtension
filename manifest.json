  {
    "name": "CalCentralCalendar Chrome Extension",
    "version": "0.0.1",
    "description": "Quickly convert your CalCentral schedule to Google Calendar events!",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "images/calendar128.png"
      }
    },
    "icons": {
      "128": "images/calendar128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
  }